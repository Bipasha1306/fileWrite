DELETE FROM Employees
WHERE EmployeeID NOT IN (
    SELECT MIN(EmployeeID)
    FROM Employees
    GROUP BY Email
);
/////////
DELETE FROM Employees
WHERE EmployeeID IN (
    SELECT EmployeeID
    FROM (
        SELECT EmployeeID,
               ROW_NUMBER() OVER (PARTITION BY Email ORDER BY EmployeeID) AS rn
        FROM Employees
    ) AS RankedEmails
    WHERE rn % 2 = 0
);

