DELETE FROM Employees
WHERE EmployeeID NOT IN (
    SELECT MIN(EmployeeID)
    FROM Employees
    GROUP BY Email
);
/////////
DELETE FROM marsods.MARS_RADIX_QUERY_DTLS_DUMMY
WHERE (QUERY_ID, ENTITY_ID) IN (
    SELECT QUERY_ID, ENTITY_ID
    FROM (
        SELECT QUERY_ID, ENTITY_ID, 
               ROW_NUMBER() OVER (PARTITION BY QUERY_ID ORDER BY ENTITY_ID) AS rn 
        FROM marsods.MARS_RADIX_QUERY_DTLS_DUMMY 
        WHERE ENTITY_NAME = 'Party'
    )
    WHERE MOD(rn, 2) = 1
);

